<div id="mnplaceholder-605b42e13323e" data-size="1x1" data-aid="611817330" data-device="any" class="mnPlaceholderSync fa-hide" data-versionid="3111299" data-cid="8CUX59211"  data-url="https://contextual.media.net/nmedianet.js"></div><script>
var attributeNofollowValue = 'nofollow noopener';
(function($){
    $(function(){
        var isContributor = 1;
        function isInternalLink(url) {
            var siteUrl = 'https://www.forbes.com/advisor';

            var url_domain = url.split('/')[2];  
            if (siteUrl.indexOf(url_domain) >= 0) { 
                return true;
            } else {
                return false;
            }
        }

        $('.article-body a').each(function() {
            var linkValue = $(this).attr('href');
            if (linkValue) {
                var gaTrackingValue = "ExternalLink:" + linkValue;
                $(this).attr('ga-data-track', gaTrackingValue);

                if (isContributor && !isInternalLink(linkValue) ) {
                    $(this).attr('rel', attributeNofollowValue);
                }
            }
        });
    });
})(jQuery);
</script><script>
    (function handleExternalLinking() {
        var articleBody = '.article-link-wrapper';
        var siteUrl = 'https://www.forbes.com/advisor';
        if (document.querySelector(articleBody)) {
            document.querySelectorAll(articleBody + ' a').forEach(function(elm,index) {
                var link = elm.getAttribute('href');
                if (link.indexOf(siteUrl) == -1) {
                    elm.setAttribute('target', '_blank');
                    elm.setAttribute('rel','noopener');
                }
            });
        }
    })();
</script>    <script type="text/javascript">
        function introContentCollapsible() {
            var targetEl = '.intro-content',
                triggerEl = '.intro-content .mob-fade',
                introHt = $('.intro-content-inner').height();
            if (deviceType === "mobile" && introHt < 130) {
                $(targetEl).removeClass('applyfade');
                $(triggerEl).css('display', 'none');
            }
            $(triggerEl).on('click', function() {   
                $(targetEl).toggleClass('active');
                $('.intro-content .mob-fade span').text(function(i, text){
                    return text === "SHOW MORE" ? "SHOW LESS" : "SHOW MORE";
                })
                $('.intro-content .chevron-arrow').toggleClass('top');
            });
        }
        introContentCollapsible();
    </script>
