    var deviceType = getDeviceTypeInNumber(window.deviceType);
    var isBreakpointDesktop = (deviceType === 1 || window.innerWidth > 980);
    var posData = {
        top : {
            placeholder:'top-ed-placeholder',
            sizes:[[728,90],[970,250], [1,1], ['fluid']],
            tabletSizes:[[728,90]],
        },
        articleStream : {
            placeholder:'article-stream-ed-placeholder',
            sizes:[[300,600], [1,1], ['fluid']],
        },
        articleBlock : {
            placeholder:'article-block-ed-placeholder',
            sizes:[[300,250], [300,600], [1,1], ['fluid']],
        },
        aboveFaqs : {
            placeholder:'above-faqs-ed-placeholder',
            sizes:[[300,250], [1,1], ['fluid']],
        },
        above101Section : {
            placeholder:'above-101-section-ed-placeholder',
            sizes:[[300,250], [1,1], ['fluid']],
        },
        aboveArticleStream : {
            placeholder:'above-article-stream-ed-placeholder',
            sizes:[[300,250], [1,1], ['fluid']],
        },
    };

    var selectors = {
        topEd: '.top-ed-placeholder',
        header: '.header-us'
    };

    //handle sticky top
    var isSticky = false;
    var isStickyDone = false;
    function handleTopEdScroll(){
        var topEd = document.querySelector(selectors.topEd);
        var headerHeight = document.querySelector(selectors.header).offsetHeight;
        var offset = window.pageYOffset || document.documentElement.scrollTop;
        if(topEd){
            var scrollOffset = topEd.offsetTop;
            if(!isSticky){
                if(topEd.classList.contains('top-ed-placeholder--sticky')){
                    isSticky = true;
                }
                if( scrollOffset > offset ){
                    topEd.classList.add('top-ed-placeholder--sticky');
                }
                setTimeout(function(){
                    topEd.classList.remove('top-ed-placeholder--sticky');
                    isStickyDone = true;
                },5000);
            }else{
                if(!isStickyDone){
                    topEd.style.top = (this.oldScroll > this.scrollY)? headerHeight+"px" : "0px";
                }
            }
        }
        this.oldScroll = this.scrollY;
    }

    function throttle(fn, delay) {
        var canCall = true;
        return function () {
            if (canCall) {
                for (var _len = arguments.length, args = new Array(_len), _key = 0; _key < _len; _key++) {
                    args[_key] = arguments[_key];
                }

                fn.apply(null, args);
                canCall = false;
                setTimeout(function () {
                    canCall = true;
                }, delay);
            }
        };
    };
    
    window.addEventListener('scroll',throttle(handleTopEdScroll,250));

    /**
     * Set the placeholder height of lazy loaded units
     */
    function setMobilePlaceholdersHeight() {
        googletag.cmd.push(function () {
            googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                var slotEl = document.querySelector('#' + event.slot.getSlotElementId()).parentElement;
                
                if (!!slotEl) {
                    slotEl.style.minHeight = 'initial';
                }
            }); 
        });
    }

    //dfp setup
    window.dfpConfig = {
        1: {
            'recx' : {
                'size': posData.articleStream.sizes,
                'cssClass': posData.articleStream.placeholder
            },
            'top' : {
                'size': posData.top.sizes,
                'cssClass': posData.top.placeholder
            },
            'rec' : {
                'size': posData.articleBlock.sizes,
                'cssClass': posData.articleBlock.placeholder
            },
        },
        2: {
            'recx' : {
                'size': posData.articleStream.sizes,
                'cssClass': posData.articleStream.placeholder
            },
            'top' : {
                'size': posData.top.tabletSizes,
                'cssClass': posData.top.placeholder
            },
            'rec' : {
                'size': posData.articleBlock.sizes,
                'cssClass': posData.articleBlock.placeholder
            },
        },
        3: {
            'mobilerec': {
                'size': posData.aboveFaqs.sizes,
                'cssClass': posData.aboveFaqs.placeholder
            },
            'mobilex' : {
                'size': posData.above101Section.sizes,
                'cssClass': posData.above101Section.placeholder
            },
            'inread' : {
                'size': posData.aboveArticleStream.sizes,
                'cssClass': posData.aboveArticleStream.placeholder
            },
        }
    };
    if (!isBreakpointDesktop) {
        window.dfpConfig[2] = {
            'mobilerec': {
                'size': posData.aboveFaqs.sizes,
                'cssClass': posData.aboveFaqs.placeholder
            },
            'mobilex' : {
                'size': posData.above101Section.sizes,
                'cssClass': posData.above101Section.placeholder
            },
            'inread' : {
                'size': posData.aboveArticleStream.sizes,
                'cssClass': posData.aboveArticleStream.placeholder
            },
        }
    }
    generatePlaceHolders(null);
    initializeDFP(function(){
        if ( isBreakpointDesktop ) { // desktop and big tablet
            loadSingleUnit(['top', 'rec', 'recx'], 0, null);
        } else{ // mobile and small tablet
            var mobEdsArr = ['mobilerec', 'inread'];
            if (document.querySelector('.' + posData.above101Section.placeholder)) {
                mobEdsArr.push('mobilex');
            }

            // if (deviceType === 3 ) { // mobile
            //     if (document.querySelector('.' + posData.above101Section.placeholder)) {
            //         mobEdsArr.push('mobilex');
            //     }
            // } else { // small tablet
            //     mobEdsArr.push('top');
            // }

            loadSingleUnit(mobEdsArr, 0, null);
            setMobilePlaceholdersHeight();
        }
    });
