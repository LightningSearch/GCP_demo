<script>
(function ($) {
    $(document).ready(function(){  
        var device_type = getDeviceTypeInNumber(window.deviceType);

        $( ".template-wrapper" ).click(function(e) {
            if (e.target.id !== 'advertiser-disclosure') {
                $('#advertiser-disclosure-block').fadeOut();
            }
        });
        $( window ).scroll(function(e) {
            if (e.target.id !== 'advertiser-disclosure') {
                if(device_type === 3){
                    $('#advertiser-disclosure-block').fadeOut();
                }
                else{
                    if(device_type === 3 && ($(window).scrollTop() > $("#advertiser-disclosure-block").outerHeight() + $("#advertiser-disclosure").offset().top)){
                        $('#advertiser-disclosure-block').fadeOut();
                    }
                }
                
            }
        });
        if(device_type === 3){
            var discBlock = document.getElementById('advertiser-disclosure-block');
            $(discBlock).css('width', '96%'); // dynamic disclosure block width for mobile devices
            $(discBlock).css('height', '255px');
            if(document.documentElement.clientHeight < document.documentElement.clientWidth && document.documentElement.clientWidth < 600){
                $(discBlock).css('height', '225px');
            }
            new SimpleBar(discBlock, { autoHide: false});
        }
    }); 
})(jQuery);
</script><script>
    (function handleExternalLinking() {
        var articleBody = '.article-link-wrapper';
        var siteUrl = 'https://www.forbes.com/advisor';
        if (document.querySelector(articleBody)) {
            document.querySelectorAll(articleBody + ' a').forEach(function(elm,index) {
                var link = elm.getAttribute('href');
                if (link.indexOf(siteUrl) == -1) {
                    elm.setAttribute('target', '_blank');
                    elm.setAttribute('rel','noopener');
                }
            });
        }
    })();
