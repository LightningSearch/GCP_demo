<script>
    $(function () {
        var pageNumber = 0;

        $('#more-all-articles').click(function () {
            var allArticleBtn = $(this);
            pageNumber++;

            var sectionParams = {
                action: 'load_more_articles',
                pagenumber: pageNumber,
                idsToExclude: "[86275,82943,81089,81103,79508]",
                argsCategory: "life-insurance",
                argsCount: "5",
            };

            $.ajax({
                url: faglobal.ajax_url,
                type: 'post',
                data: sectionParams,
                success: function (reponse) {
                    if (typeof reponse != 'undefined' && reponse.length != 0) {
                        var res = JSON.parse(reponse);
                        $('ul#all-articles-section').append(res.html);

                        // Hide the All Articles button
                        if (pageNumber >= res.maxcount) {
                            allArticleBtn.hide();
                        }
                    } else {
                        allArticleBtn.hide();
                    }
                },
                error: function (err) {
                    console.error('There was some problem with fetching the all related articles', err);
                }
            });
        });
    })
</script>        <div class="right-rail">
