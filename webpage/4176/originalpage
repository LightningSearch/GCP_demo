     data-hydro-click="{&quot;event_type&quot;:&quot;code_navigation.click_on_symbol&quot;,&quot;payload&quot;:{&quot;action&quot;:&quot;click_on_symbol&quot;,&quot;repository_id&quot;:184657328,&quot;ref&quot;:&quot;release/5.1&quot;,&quot;language&quot;:&quot;Markdown&quot;,&quot;originating_url&quot;:&quot;https://github.com/NVIDIA/TensorRT/tree/release/5.1/demo/BERT&quot;,&quot;user_id&quot;:null}}"
     data-hydro-click-hmac="7b3b9a611ea62f378e1773584761ce72c290b740c2a779d6e0d0618ae55cd302">
  <div class="Popover-message Popover-message--large Popover-message--top-left TagsearchPopover mt-1 mb-4 mx-auto Box color-shadow-large">
