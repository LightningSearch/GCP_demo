<script>
	function injectScript(){
        window.EventBus.$on('page_vue_mounted', initDrawer)
    }

    function initDrawer(){
        var button = document.querySelector('.overlay-button');
        var text = document.querySelector('.overlay-text');

        button.addEventListener("click", function(ele){
           ele.target.style.display = 'none'
           text.childNodes.forEach(function(e) {
                if (e.classList !== undefined) {
                    e.classList.remove('overlay');
                    e.classList.remove('hidden');
                }
           })
        });
    }

    window._mpScripts.push(injectScript);
</script>

	
