                         v-on:click="onDropdownClick('pros-cons')"
                         
                    >
                        <div class="flex justify-between items-center">
                            <span class="font-medium text-sm leading-24px text-395BB6">Pros & Cons</span>
                            <span ref="pros-cons-svg"
                                  
